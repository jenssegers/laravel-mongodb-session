{
    "name": "ionutgrecu/mongodb-session",
    "description": "A MongoDB session driver for Laravel 4, 5, 6, 7, 8 and 9",
    "keywords": ["laravel","session","mongodb","mongo","driver"],
    "authors": [
        {
            "name": "Jens Segers",
            "homepage": "http://jenssegers.be"
        }
    ],
    "license" : "MIT",
    "require": {
        "jenssegers/mongodb": "*",
        "illuminate/session": "~4.0|~5.0|~6.0|~7.0|~8.0|~9.0"
    },
    "autoload": {
        "psr-0": {
            "Jenssegers\\Mongodb\\Session": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jenssegers\\Mongodb\\Session\\SessionServiceProvider"
            ]
        }
    }
}
